                  <m:mi>t</m:mi>
                  <m:mi>t</m:mi>
                  <m:mi>e</m:mi>
                  <m:mi>r</m:mi>
                  <m:mi>y</m:mi>
                </m:mrow>
              </m:msub>
              <m:mo>=</m:mo>
              <m:msub>
                <m:mi>V</m:mi>
                <m:mrow>
                  <m:mi>t</m:mi>
                  <m:mi>o</m:mi>
                  <m:mi>t</m:mi>
                  <m:mi>a</m:mi>
                  <m:mi>l</m:mi>
