        </equation>
        <!--empty paragraphs get left behind.-->
        <equation id="id63774">
          <m:math overflow="scroll" mode="display">
            <m:mtable displaystyle="true">
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:mrow>
                    <m:mi> Displacement </m:mi>
                    <m:mo>(</m:mo>
                    <m:mi>Δ</m:mi>
