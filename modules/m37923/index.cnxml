              </m:mtr>
            </m:mtable>
          </m:math>
        </equation>
        <!--empty paragraphs get left behind.-->
        <equation id="id63551">
          <m:math overflow="scroll" mode="display">
            <m:mtable displaystyle="true">
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:mrow>
                    <m:mi> Displacement </m:mi>
                    <m:mo>(</m:mo>
                    <m:mi>Δ</m:mi>
                    <m:mi mathvariant="normal">x</m:mi>
                    <m:mo>)</m:mo>
                  </m:mrow>
                </m:mtd>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:msub>
                      <m:mi>x</m:mi>
                      <m:mi>f</m:mi>
                    </m:msub>
                    <m:mspace width="3.33333pt"/>
                    <m:mo>-</m:mo>
                    <m:mspace width="3.33333pt"/>
                    <m:msub>
                      <m:mi>x</m:mi>
