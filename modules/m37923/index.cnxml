                  <m:mrow>
                    <m:msub>
                      <m:mi>x</m:mi>
                      <m:mi>f</m:mi>
                    </m:msub>
                    <m:mspace width="3.33333pt"/>
                    <m:mo>-</m:mo>
                    <m:mspace width="3.33333pt"/>
