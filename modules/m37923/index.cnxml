                  </m:mtd>
                </m:mtr>
                <m:mtr>
                  <m:mtd columnalign="right">
                    <m:mi>t</m:mi>
                  </m:mtd>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mo>?</m:mo>
                      <m:mrow><m:mspace width="1.em"/>
                        <m:mi> at </m:mi><m:mspace width="2pt"/>
                        <m:mi> half </m:mi><m:mspace width="2pt"/>
                        <m:mi> the </m:mi><m:mspace width="2pt"/>
                        <m:mi> distance </m:mi>
                      </m:mrow>
                      <m:mi>Δ</m:mi>
                      <m:mi mathvariant="normal">x</m:mi>
                      <m:mspace width="3.33333pt"/>
                      <m:mo>=</m:mo>
                      <m:mspace width="3.33333pt"/>
                      <m:mn>32</m:mn>
                      <m:mspace width="3.33333pt"/>
                      <m:mi mathvariant="normal">m</m:mi>
