                      <m:mspace width="4pt"/>
                      <m:mi> rectangle </m:mi>
                    </m:mrow>
                  </m:mtd>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mi>a</m:mi>
                      <m:mo>×</m:mo>
                      <m:mi>Δ</m:mi>
                      <m:mi>t</m:mi>
                    </m:mrow>
                  </m:mtd>
